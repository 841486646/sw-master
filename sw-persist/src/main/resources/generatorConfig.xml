<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<properties resource="jdbc.properties" />
    <context id="managementTables" targetRuntime="MyBatis3">
		<property name="javaFileEncoding" value="UTF-8"/>
		<plugin type="com.swtec.sw.persist.mybatis.plugin.PageExtPlugin"/> 
        <commentGenerator>
            <property name="suppressAllComments" value="false" />
            <property name="suppressDate" value="true" />
        </commentGenerator>

        <jdbcConnection driverClass="${jdbc.driver}"
            connectionURL="${jdbc.url}" 
            userId="${jdbc.username}" 
            password="${jdbc.password}" />

        <!-- 下面的都不用修改 -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>
        
        <!-- generate Model -->
        <javaModelGenerator targetPackage="com.swtec.sw.persist.model" targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        
        <!-- generate xml -->
        <sqlMapGenerator targetPackage="sqlmap"  targetProject="src/main/resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>
        
        <!-- generate Mapper -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.swtec.sw.persist.mapper"  targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>
        
        <!-- 规则信息表 -->
        <table schema="" tableName="sw_receivables" domainObjectName="Receivables">
            <generatedKey column="id" sqlStatement="JDBC" identity="true"/>
        </table>
    </context>
</generatorConfiguration>